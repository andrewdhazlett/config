[user]
	name = andrewdhazlett
	email = andrew@hazlett.me
[credential]
	helper = store
[credential "https://github.com"]
	username = andrewdhazlett
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[alias]
  a = add
	ac = commit --amend
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
	po = push origin
	ph = push origin HEAD
[github]
	user = andrewdhazlett
